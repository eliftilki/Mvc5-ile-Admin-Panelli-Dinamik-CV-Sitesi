@using WebApplication4.Models.Entity
@model List<TblYeteneklerim>

<div class="resume-section-content">
    <h2 class="mb-5">yeteneklerim</h2>
    <div class="subheading mb-3">Programlama Dilleri</div>
    <ul class="list-inline dev-icons">
        <li class="list-inline-item"><i class="fab fa-java"></i></li>
        <li class="list-inline-item"><i class="fab fa-git "></i>
        <li class="list-inline-item"><i class="fab fa-html5 "></i>
        <li class="list-inline-item"><i class="fab fa-css3 "></i>
        <li class="list-inline-item"><i class="fab fa-python"></i></li>
        
    </ul>
    <div class="subheading mb-3">Becerilerim</div>
    <ul class="fa-ul mb-0">
        @foreach(var x in Model)
         {
        <li>
            <span class="fa-li"><i class="fas fa-check"></i></span>
            @x.Yetenek
        </li>
        }
    </ul>
</div>
